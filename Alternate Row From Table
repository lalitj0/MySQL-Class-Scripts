show DATABASES;
CREATE DATABASE expt_3;
USE expt_3;
CREATE TABLE students(
 id INT NOT NULL AUTO_INCREMENT,
 first_name VARCHAR (255) NOT NULL,
 age INT NOT NULL,
 PRIMARY KEY (id)
 );
 INSERT INTO students ( first_name,age) VALUES
 ('X',20),
 ('Y',21),
 ('y',21),
 ('z',22),
 ('XY',22);
 SELECT * FROM students;
 SELECT*FROM students WHERE id % 2 = 0;
